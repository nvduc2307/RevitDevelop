<Window x:Class="RevitDevelop.Tools.InstallConstructionJoinOfWalls.views.SelectConstructionJionInCad"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RevitDevelop.Tools.InstallConstructionJoinOfWalls.views"
        mc:Ignorable="d"
        SizeToContent="WidthAndHeight"
        Title="SelectConstructionJionInCad">
    <Window.Resources>
        <ResourceDictionary>
            <Style TargetType="ComboBox">
                <Setter Property="Height" Value="25"/>
            </Style>
            <Style TargetType="Button">
                <Setter Property="Height" Value="25"/>
            </Style>
        </ResourceDictionary>
    </Window.Resources>
    <StackPanel Margin="15">
        <StackPanel>
            <TextBlock Text="Selecte Cad File:"/>
            <ComboBox/>
        </StackPanel>
        <StackPanel Margin="0,5,0,0">
            <TextBlock Text="Selecte Construction Join Layer:"/>
            <ComboBox/>
        </StackPanel>
    </StackPanel>
</Window>
